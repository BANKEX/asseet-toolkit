      //mt-tab(title="Dividends")
        .tokens-info.no_tokens(*ngIf="!tokens") You haven’t got any tokens.
        .tokens-list(*ngIf="tokens")
          perfect-scrollbar
            .token(*ngFor="let key of objectKeys(dividends) | orderBy:sortBy.id:sortBy.reverse")
              .token-number
                a((click)="showDividendsTransactions(key)", href="javascript:void(0)", title="Show") type {{key}}
              .token-sum {{dividends[key]}} Wei
              //-.token-sum(*ngIf="tokens[key].pending.length>0") Blocked: {{calculatePendings(tokens[key])}} tokens
              //-  img(src="/assets/spinner.gif", alt="transaction in process")
              .token-controls
                span.token-transfer((click)="openWithdrawModal(key, tokens[key])", title="Withdraw dividends")
                  <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16">
